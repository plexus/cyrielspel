{:deps
 {:aliases [:dev]}

 :dev-http
 {8000 "classpath:public"}

 :builds
 {:main
  {:target     :browser
   :modules    {:main {:entries [trypixi.core]}}
   :output-dir "resources/public/ui"
   :asset-path "/ui"
   :devtools   {:infer-externs :auto
                :repl-pprint true
                :warnings-as-errors #{:undeclared-var}}}}}
